{
  "name": "@app/web",
  "version": "0.0.0",
  "private": true,
  "description": "React FE based on AntD Pro that integrates with graphile starter",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "dev": "npm run start:dev",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "site": "npm run fetch:blocks && npm run build",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none PORT=8001 GRAPHQL_URL=http://localhost:3001/graphql UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "4.6.2",
    "@ant-design/pro-descriptions": "1.8.1",
    "@ant-design/pro-form": "1.28.1",
    "@ant-design/pro-layout": "6.19.4",
    "@ant-design/pro-list": "1.9.1",
    "@ant-design/pro-table": "2.39.1",
    "@apollo/client": "3.3.20",
    "@umijs/route-utils": "1.0.37",
    "ahooks": "2.10.6",
    "antd": "4.16.2",
    "antd-password-input-strength": "1.1.1",
    "classnames": "2.3.1",
    "dayjs": "1.10.5",
    "dotenv": "10.0.0",
    "lodash": "4.17.21",
    "omit.js": "2.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "recoil": "0.3.1",
    "umi": "3.4.25",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "2.1.1",
    "@types/classnames": "2.3.1",
    "@types/express": "4.17.12",
    "@types/history": "4.7.8",
    "@types/jest": "26.0.23",
    "@types/lodash": "4.14.170",
    "@types/react": "17.0.11",
    "@types/recoil": "0.0.9",
    "@types/uuid": "8.3.0",
    "@umijs/fabric": "2.6.2",
    "@umijs/plugin-blocks": "2.2.2",
    "@umijs/plugin-esbuild": "1.3.1",
    "@umijs/preset-ant-design-pro": "1.3.2",
    "@umijs/preset-dumi": "1.1.19",
    "@umijs/preset-react": "1.8.12",
    "@umijs/yorkie": "2.0.5",
    "carlo": "0.9.46",
    "cross-env": "7.0.3",
    "cross-port-killer": "1.3.0",
    "detect-installer": "1.0.2",
    "enzyme": "3.11.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint": "7.28.0",
    "express": "4.17.1",
    "graphql": "15.5.0",
    "lint-staged": "11.0.0",
    "mockjs": "1.1.0",
    "prettier": "2.3.1",
    "prettier-plugin-style-order": "0.2.2",
    "react-dev-inspector": "1.6.0",
    "puppeteer-core": "10.0.0",
    "stylelint": "13.13.1",
    "typescript": "4.3.2"
  },
  "engines": {
    "node": ">=14.15.4"
  },
  "create-umi": {
    "ignoreScript": [
      "docker*",
      "functions*",
      "site",
      "generateMock"
    ],
    "ignoreDependencies": [
      "netlify*",
      "serverless"
    ],
    "ignore": [
      ".dockerignore",
      ".git",
      ".github",
      ".gitpod.yml",
      "CODE_OF_CONDUCT.md",
      "Dockerfile",
      "Dockerfile.*",
      "lambda",
      "LICENSE",
      "netlify.toml",
      "README.*.md",
      "azure-pipelines.yml",
      "docker",
      "CNAME",
      "create-umi"
    ]
  }
}
